package UD04_R1;

public class R1_ejer_01 {
	public static void main(String[] args) {
		int i;
		Bombilla b = new Bombilla(); 
		// Creamos un objeto bombilla 
		// La encendemos y apagamos 1000 veces 
		for(i=1; i<=1000;i++) 
		{ 
		b.encender(); 
		b.apagar(); 
		} 
		// Y otra vez más, aunque ya debe estar fundida b.encender(); 
		b.apagar(); 

	}

}
